class Visualizer
{
private:
    const std::string menu_name;
    const std::string worldfile_name;

public:
    Visualizer(const std::string &menu_name, const std::string &worldfile_name)
        : menu_name(menu_name), worldfile_name(worldfile_name)
    {
    }


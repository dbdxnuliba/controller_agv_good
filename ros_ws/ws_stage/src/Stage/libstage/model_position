void print_trace()
{

    void *stack_trace[16] = {0};
    char **stack_strings = 
# 16 "/home/zhou/work/codes/Stage/libstage/common.h" 3 4
                          __null
# 16 "/home/zhou/work/codes/Stage/libstage/common.h"
                              ;
    int stack_depth = 0;
    int i = 0;


    stack_depth = backtrace(stack_trace, 16);


    stack_strings = (char **)backtrace_symbols(stack_trace, stack_depth);
    if (
# 25 "/home/zhou/work/codes/Stage/libstage/common.h" 3 4
       __null 
# 25 "/home/zhou/work/codes/Stage/libstage/common.h"
            == stack_strings) {
        printf(" Memory is not enough while dump Stack Trace! \r\n");
        return;
    }


    printf(" Stack Trace: \r\n");
    for (i = 0; i < stack_depth; ++i) {
        printf(" [%d] %s \r\n", i, stack_strings[i]);
    }


    free(stack_strings);
    stack_strings = 
# 38 "/home/zhou/work/codes/Stage/libstage/common.h" 3 4
                   __null
# 38 "/home/zhou/work/codes/Stage/libstage/common.h"
                       ;

    return;
}
